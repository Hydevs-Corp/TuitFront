{
    "name": "renforcement-js-solo",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "build": "vue-tsc -b && vite build",
        "preview": "vite preview",
        "build:w": "nodemon -e vue,ts --exec \"npm run build || exit 1\"",
        "test:cypress": "cypress run",
        "test:vitest": "vitest run",
        "test": "npm run test:cypress && npm run test:vitest"
    },
    "dependencies": {
        "pinia": "^2.2.2",
        "vue": "^3.4.37",
        "vue-router": "^4.4.5"
    },
    "devDependencies": {
        "@types/cypress": "^0.1.6",
        "@vitejs/plugin-vue": "^5.1.2",
        "cypress": "^13.14.2",
        "dotenv": "^16.4.5",
        "nodemon": "^3.1.5",
        "typescript": "^5.5.3",
        "vite": "^5.4.1",
        "vitest": "^2.1.1",
        "vue-tsc": "^2.0.29"
    }
}
